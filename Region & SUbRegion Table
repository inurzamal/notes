CREATE MULTISET TABLE D_EIW_ERA_FRR_OLTP.CountryRegion_Table
  NO FALLBACK,
  NO BEFORE JOURNAL,
  NO AFTER JOURNAL,
  CHECKSUM = DEFAULT,
  DEFAULT MERGEBLOCKRATIO,
  MAP = TD_MAP1
(
    Region VARCHAR(25) NOT NULL,
    Region_Code VARCHAR(3) NULL,
    Comment VARCHAR(50) NULL
)
UNIQUE PRIMARY INDEX (Region);

---

CREATE MULTISET TABLE D_EIW_ERA_FRR_OLTP.CountrySubregion_Table
  NO FALLBACK,
  NO BEFORE JOURNAL,
  NO AFTER JOURNAL,
  CHECKSUM = DEFAULT,
  DEFAULT MERGEBLOCKRATIO,
  MAP = TD_MAP1
(
    Subregion VARCHAR(25) NOT NULL,
    Subregion_Code VARCHAR(3) NULL,
    Comment VARCHAR(50) NULL
)
UNIQUE PRIMARY INDEX (Subregion);
